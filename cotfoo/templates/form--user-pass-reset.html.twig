<section class="log-wrapper">
    <div class="container">
        <div class="col-md-6 col-sm-10 col-md-offset-3 col-sm-offset-1">
            <div class="log-box login-box ">
                <form class="{{ attributes }}">
                    <div class="alert alert-success alert-dismissable">        
                        {{element.message}}
                    </div>
                    <div class="text-center create-acnt">
                        {{element.actions.submit}}
                    </div>
                    {{ element.form_build_id }} {# required #}
                    {{ element.form_id }} {# required #}
                    {{ element.form_token }}{# required #}
                </form>
            </div>
        </div>
    </div>
</section>