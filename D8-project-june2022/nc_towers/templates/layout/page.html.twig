<div class="layout-container">
    {% if page.top_header %}
    <div class="top-banner-header">
      {# {{ page.top_header }} #}
    </div>
    {% endif %}
   <div class="header_bg_main">
    {% include '@nc_towers/includes/header.html.twig' %}
    <div>
      {{ page.secondary_menu }}
    </div>
    {{ page.breadcrumb }}
    {{ page.highlighted }}
    {{ page.help }}
    <main role="main" class="main-content-block">
      <a id="main-content" tabindex="-1"></a>{# link is in html.html.twig #}

      <div class="secondary-header">
        {{ page.secondaryHeader }}
      </div>{# /.secondary-header #}

      <div class="layout-content occupation-search page-title">
      {{ page.PageTitle }}

      </div>
      
    <div id="occupation_page_print">
      <div class="layout-content-right-left">
        <div class="layout-right-content">
          {{ page.contentRight }}
        </div>
        <div class="layout-left-content">
          {{ page.contentLeft }}
        </div>
      </div>

      <div class="layout-content-full">
        {{ page.contentFull }}
      </div>

      <div class="layout-content">
        <div class="left-content">
          {% if page.sidebar_first %}
            <aside class="layout-sidebar-first" role="complementary">
              {{ page.sidebar_first }}
            </aside>
          {% endif %}
        </div>
        <div class="center-content">
          {{ page.content }}
        </div>
        <div class="right-content">
          {% if page.sidebar_second %}
            <aside class="layout-sidebar-second" role="complementary">
              {{ page.sidebar_second }}
            </aside>
          {% endif %}
        </div>
      </div>{# /.layout-content #}
    </div>
    <div id='occupation_detail_print' style='display:none;'></div>
    </main>

    {% if is_front %}
    {% include '@nc_towers/includes/home-bottom-blocks.html.twig' %}
    <!-- Bottom bg -->
    <div class="bottom-bg"></div>
    <!-- Bottom bg -->
    {% endif %}

    {% if page.footer %}
    {% include '@nc_towers/includes/footer.html.twig' %}
    {% endif %}
    {{ page.stickRight }}
