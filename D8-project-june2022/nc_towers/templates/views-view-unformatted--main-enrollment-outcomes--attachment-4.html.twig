
<div class="sector-item {{title|striptags|trim|convert_encoding('UTF-8', 'HTML-ENTITIES')|replace({"'": ""}) }}" data-sector="{{ title }}">
{% for row in rows %}
  {%
    set row_classes = [
      default_row_class ? 'views-row',
    ]
  %}
  <div{{ row.attributes.addClass(row_classes) }} data-index="{{ loop.index }}">
    {{- row.content -}}
    {# .addClass('sector-'~title) #}
  </div>
{% endfor %}
</div>