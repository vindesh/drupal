{%
  set classes = [
    'view',
    'view-' ~ id|clean_class,
    'view-id-' ~ id,
    'view-display-id-' ~ display_id,
    dom_id ? 'js-view-dom-id-' ~ dom_id,
    'container',
  ]
%}



<div{{ attributes.addClass(classes) }}>
  {{ title_prefix }}
  {{ title }}
  {{ title_suffix }}

  {% if rows %}
    <div class="views-rows">{{ rows }}</div>
  {% endif %}
  {% if empty %}
    <div class="views-empty">{{ empty }}</div>
  {% endif %}
  
  {{ pager }}
</div>
