{#
/**
 * @file
 * Display an iframe with alterable components.
 */
#}
<div class="hytPlayerWrapOuter">
	<div class="hytPlayerWrap">
		<iframe {{ attributes }} {% if url is not empty %} src="{{ url }}?rel=0&enablejsapi=1&autoplay=1&mute=1&loop=1" {% endif %} allow="accelerometer; autoplay; encrypted-media; gyroscope; picture-in-picture" >
		</iframe>
	</div>
</div>