{%
  set classes = [
    'block',
    'block-' ~ configuration.provider|clean_class,
    'block-' ~ plugin_id|clean_class,
  ]
%}



<div{{ attributes.addClass(classes) }}>
  {{ title_prefix }}
  {% if label %}
    <div class="block__title">{{ label }}</div>
    <div class="overview-link">
      <a href="{{ path('entity.node.canonical', {'node': 376}) }}">{{ 'Overview'|t }} <i class="fa fa-angle-right"></i></a>
    </div>
  {% endif %}
  {{ title_suffix }}
  {% block content %}
    {{ content }}
  {% endblock %}
</div>
